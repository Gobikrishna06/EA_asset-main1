@model Catagorytask.Models.AllCatagory_Table

@{
    Layout = null;
}




@using (Html.BeginForm("Details", "Home", FormMethod.Post, new { onsubmit = "return SubmitForm(this)" }))
{
    @Html.HiddenFor(Model => Model.Id)
<p>
    <div class="form-group">
        @Html.LabelFor(model => model.Status_report, htmlAttributes: new { @class = "control-label col-md-2", style = "font-size: 16px; font-weight: 600; color: #000" })
        <br />
        <div class="col-md-10">
            <select name="Status_report" class="form-control" id="lang">
                <option value="Active">Active</option>
                <option value="Inactive">Inactive</option>
            </select>

            @Html.ValidationMessageFor(model => model.Status_report, "", new { @class = "text-danger" })
        </div>
    </div>
    <div class="form-group">
        <input type="submit" value="submit" class="btn btn-primary" />
        <input type="reset" value="Reste" class="btn btn-warning" />
    </div>
</p>
}

<div>
    @Html.ActionLink("Back to List", "Index")
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
